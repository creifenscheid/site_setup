lib.footerMenuProcessor < lib.boilerplates.menuProcessor
lib.footerMenuProcessor {
    if.isTrue = {$plugin.tx_sitesetup.view.footerMenuParentPageId}
    special.value = {$plugin.tx_sitesetup.view.footerMenuParentPageId}
}

lib.footerTemplate < lib.boilerplates.fluidtemplate
lib.footerTemplate {
    if.isTrue = {$plugin.tx_sitesetup.view.footerTemplateName}
    templateName = {$plugin.tx_sitesetup.view.footerTemplateName}
    
    dataProcessing {
        10 < lib.footerMenuProcessor
    }
}