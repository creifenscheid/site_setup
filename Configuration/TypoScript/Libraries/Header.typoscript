lib.headerTemplate < lib.boilerplates.fluidtemplate
lib.headerTemplate {
    if.isTrue = {$plugin.tx_sitesetup.settings.headerTemplateName}
    templateName = {$plugin.tx_sitesetup.settings.headerTemplateName}
    
    dataProcessing {
        10 < lib.boilerplates.fileProcessor
    }

    variables {
        pageTitle = TEXT
        pageTitle.data = page : title
    }
}