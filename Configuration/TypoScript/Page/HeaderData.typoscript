page {
	headerData {
		
		# BASE URL
		10 = TEXT
		10 {
			data = site:base
			dataWrap = <base href="|" />
		}
		
		# FAVICON
		20 = TEXT
		20 {
			if.isTrue = {$plugin.tx_sitesetup.favicon}
			value (
				<link rel="shortcut icon" href="{$plugin.tx_sitesetup.favicon}" type="image/x-icon">
			)
		}
	}
}