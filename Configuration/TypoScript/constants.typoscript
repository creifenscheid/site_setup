    footerMenuStorage =

    # cat=Site setup/Language/a; type=string; label=LLL:EXT:site_setup/Resources/Private/Language/ConstantEditor/locallang.xlf:constantEditor.language.languageIds
    languageIds =

    # cat=Site setup/Language/b; type=string; label=LLL:EXT:site_setup/Resources/Private/Language/ConstantEditor/locallang.xlf:constantEditor.language.languageMenuTemplateName
    languageMenuTemplateName = LanguageMenu

    # customsubcategory=webapp=LLL:EXT:site_setup/Resources/Private/Language/ConstantEditor/locallang.xlf:constantEditor.customsubcategory.webapp
    webapp {
      # cat=Site setup/webapp/a; type=string; label=LLL:EXT:site_setup/Resources/Private/Language/ConstantEditor/locallang.xlf:constantEditor.webapp.appTitle
