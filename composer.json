{
	"name" : "creifenscheid/site-setup",
	"type" : "typo3-cms-extension",
	"description" : "TYPO3 extension with basic setup",
	"license" : "GPL-2.0-or-later",
	"require" : {
		"typo3/cms-core" : ">12.4.7 <=12.4.99",
		"typo3/cms-rte-ckeditor" : ">12.4.7 <=12.4.99",
		"b13/container" : "^2.1",
		"ext-pdo": "*"
	},
	"autoload" : {
		"psr-4" : {
			"CReifenscheid\\SiteSetup\\" : "Classes/"
		}
	},
	"minimum-stability" : "stable",
	"authors" : [{
			"name" : "Christian Reifenscheid",
			"email" : "dev@christian-reifenscheid.de"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "site_setup"
		}
	}
}
